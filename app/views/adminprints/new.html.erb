<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Add New Print</h1>
  <%= simple_form_for @print, url: adminprints_path do |f| %>
    <%= f.input :venue %>
    <%= f.input :location %>
    <%= f.input :artist %>
    <%= f.input :date, type: "datetime-local"  %>
    <br />
    <%= f.input :poster %>
    <%= f.submit 'Create', class: 'btn btn-primary' %>
  <% end %>
</div>