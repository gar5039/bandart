<br />
<div class="container-fluid">
 
  <div class="card-columns">

    <% @prints.each do |print| %>

    <div class="card-responsive hovereffect">
      <%= image_tag print.poster, class: 'img-fluid' %>
      <div class="overlay">
        <%= link_to 'Print Details', print_path(print) %>
        <div class="card-block">
          <p class="card-title">Venue: <%= print.venue %></p>
          <p class="text">Location: <%= print.location %></p>
          <p class="text">Date: <%= print.date %></p>
          <p class="text">Artist: <%= print.artist %></p>
        </div>
      </div>
    </div>

    <% end %>
  </div>
</div>