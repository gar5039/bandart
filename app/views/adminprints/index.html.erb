<table class="table">

<% @prints.each do |print| %>
  <tr>
    <td>
      <%= image_tag print.poster, class: 'img-thumbnail' %>
    </td>
    <td>
      <%= print.venue %>
    </td>
    <td>
      <%= print.location %>
    </td>
    <td>
      <%= print.date %>
    </td>
    <td>
      <%= print.artist %>
    </td>
    <td>
      <h1><%= link_to print.poster, adminprint_path(print), class: "btn btn-secondary" %></h1><br />
    </td>
  </tr>

<% end %>
</table>

<pre>
  <!-- <%= @prints.to_yaml %> -->
</pre>